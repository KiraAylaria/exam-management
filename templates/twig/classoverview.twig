<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>{{pageTitle}}</title>

  {{ block("css", "layout.twig") }}
</head>
<body class="hold-transition sidebar-mini layout-fixed">
<div class="wrapper">

  {{ block("preloader", "layout.twig") }}
  {{ block("navbar", "layout.twig") }}
  {{ block("sidebar", "layout.twig") }}

  <div class="content-wrapper">
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0">Klassenübersicht</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="?page=dashboard">Home</a></li>
              <li class="breadcrumb-item active">Klassenübersicht</li>
            </ol>
          </div>
        </div>
      </div>
    </div>
    <section class="content">
      <div class="container-fluid">
        <div class="card card primary">
          <div class="card-header" style="background-color: #EE4D00">
          </div>
          <div class="card-body">
            <div class="row">



          </div>
        </div>
        </div>
      </div>
    </section>
  </div>
  {{ block("footer", "layout.twig") }}
</div>
  {{ block("js", "layout.twig") }}
</body>
</html>
